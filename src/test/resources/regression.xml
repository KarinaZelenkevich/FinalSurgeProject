<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Browsers " thread-count="2" parallel="tests">
    <listeners>
        <listener class-name="tests.base.AnnotationTransformer"/>
    </listeners>
    <test name="test 1">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="tests.LoginTest"/>
            <class name="tests.ProfileTest"/>
            <class name="tests.PrintTest"/>
            <class name="tests.CalendarTest"/>
            <class name="tests.OtherCalculatorTest"/>
        </classes>
    </test>
    <test name="test 2">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="tests.WorkoutReportsTest"/>
            <class name="tests.WorkoutFullAddTest"/>
            <class name="tests.WorkoutCalculatorTest"/>
        </classes>
    </test>
</suite>